{
    "private": true,
    "name": "oe-project",
    "description": "open eoffice project",
    "version": "2.0.0",
    "engines": {
        "node": "8.x"
    },
    "scripts": {
        "dev": "npm run development",
        "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "watch-poll": "npm run watch -- --watch-poll",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "prod": "npm run production",
        "postinstall": "npm run prod",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "devDependencies": {
        "@babel/plugin-proposal-class-properties": "^7.12.1",
        "@babel/preset-react": "^7.12.10",
        "axios": "^0.19.2",
        "bootstrap": "^4.5.3",
        "cross-env": "^7.0.3",
        "jquery": "^3.5.1",
        "laravel-mix": "^5.0.9",
        "lodash": "^4.17.20",
        "popper.js": "^1.12",
        "react": "^16.14.0",
        "react-dom": "^16.14.0",
        "resolve-url-loader": "^3.1.2",
        "sass": "^1.30.0",
        "sass-loader": "^8.0.0"
    },
    "dependencies": {
        "@feuer/react-tabs": "^0.1.9",
        "dotenv": "^8.2.0",
        "downloadjs": "^1.4.7",
        "js-cookie": "^2.2.1",
        "js-file-download": "^0.4.12",
        "jsonwebtoken": "^8.5.1",
        "moment": "^2.29.1",
        "react-animations": "^1.0.0",
        "react-app-polyfill": "^2.0.0",
        "react-draggable": "^4.4.3",
        "react-easy-crop": "^3.3.1",
        "react-fade-in": "^1.1.1",
        "react-flash-message": "^1.0.5",
        "react-hook-form": "^5.7.2",
        "react-image-crop": "^8.6.6",
        "react-image-lightbox": "^5.1.1",
        "react-lazyload": "^3.1.0",
        "react-loading": "^2.0.3",
        "react-loading-overlay": "^1.0.1",
        "react-lottie": "^1.2.3",
        "react-modal": "^3.12.1",
        "react-notifications-menu": "^1.0.1",
        "react-pdf": "^4.2.0",
        "react-redux": "^7.2.2",
        "react-reveal": "^1.2.2",
        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-router-hash-link": "^2.4.0",
        "react-router-transition": "^2.1.0",
        "react-sizeme": "^2.6.12",
        "react-smooth-scrollbar": "^8.0.6",
        "react-smooth-scrolling": "^1.0.2",
        "react-spinners": "^0.8.3",
        "react-toastify": "^5.5.0",
        "react-tooltip": "^4.2.13",
        "react-transition-group": "^4.4.1",
        "react-use-dimensions": "^1.2.1",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "smooth-scrollbar": "^8.6.0",
        "typewriter-effect": "^2.17.0",
        "use-error-boundary": "^2.0.4",
        "use-resize-observer": "^7.0.0",
        "uuid": "^8.3.2"
    },
    "babel": {
        "presets": [
            "@babel/preset-env",
            "@babel/preset-react"
        ],
        "plugins": [
            "@babel/plugin-proposal-class-properties"
        ]
    }
}
